! ------------------------------------------------------------------------------
! URXVT Color theme
! ------------------------------------------------------------------------------
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste

URxvt.iso14755: False
URxvt.saveLines: 5000
URxvt.letterSpace: 1
URxvt.perl-ext-common: default,clipboard,tabbed

URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 3
URxvt.tabbed.tab-bg: 0

URxvt.cursorBlink:   false
URxvt.cursorColor:    #FFFFFF
URxvt.cursorUnderline:     false
URxvt.pointerColorBackground: #586e75
URxvt.pointerColorForeground: #93a1a1
! Pointer
URxvt.pointerBlank:         true
URxvt*cursor: I
!/* Couleurs Tango */
URxvt.matcher.button: 1
URxvt.urlLauncher: chromium
URxvt.foreground: #C8C8C8
URxvt.background: #323232
URxvt.color0:     #000000
URxvt.color1:     #CC0000
URxvt.color2:     #4E9A06
URxvt.color3:     #C4A000
URxvt.color4:     #3465A4
URxvt.color5:     #75507B
URxvt.color6:     #06989A
URxvt.color7:     #D3D7CF
URxvt.color8:     #555753
URxvt.color9:     #EF2929
URxvt.color10:    #8AE234
URxvt.color11:    #FCE94F
URxvt.color12:    #729FCF
URxvt.color13:    #AD7FA8
URxvt.color14:    #34E2E2
URxvt.color15:    #EEEEEC

URxvt.transparent: true
URxvt.shading:     10
URxvt.fading:      40
URxvt*font:        xft:Hack:pixelsize=14,xft:DejaVu\ Sans\ Mono\ for\ Powerline:pixelsize=14
URxvt.scrollBar:         false
URxvt.scrollTtyOutput:   false
URxvt.scrollTtyKeypress: true

! ROFI SETUP HERE !

! Enable the extended coloring options
rofi.color-enabled: true
!                  bg       border   separator
rofi.color-window: argb:c01e1e20, argb:00274850, argb:00274850
!                  bg       fg       bg-alt   hl-bg    hl-fg
rofi.color-normal: argb:001e1e20, #c5c8c6, argb:001e1e20, argb:001e1e20, #274850
rofi.color-active: argb:001e1e20, #c5c8c6, argb:001e1e20, argb:001e1e20, #274850
rofi.color-urgent: argb:001e1e20, #c5c8c6, argb:001e1e20, argb:001e1e20, #274850

rofi.separator-style: solid
rofi.sidebar-mode: false
rofi.lines: 5
!rofi.font: Hack Regular 12
rofi.font: monofur 13
rofi.bw: 1
rofi.columns: 2
rofi.padding: 5
rofi.fixed-num-lines: true
rofi.hide-scrollbar: true

*.antialias: 1
*.autohint: 0
*.hinting: 1
hintsryle: hintfull
